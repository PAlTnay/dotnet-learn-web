@{
    ViewData["Title"] = "Movie session";
}
<form asp-action="ChangeMovieSession">
    <div class="form-group">
        <select name="Movie">
            @foreach (var movie in (ViewData["Movies"] as List<MovieModel>))
            {
                <option value="@movie.Name">@movie.Name</option>
            }
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Chnage movie session" class="btn btn-primary" />
    </div>
</form>